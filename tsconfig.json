{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "lib": ["es2020"],
    "target": "es2018",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": false,
    "newLine": "lf",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "noUnusedLocals": false,
    "declaration": true,
    "sourceMap": true,
    "outDir": "dist"
  }
}
