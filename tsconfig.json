{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "Node",
    "lib": ["ES2020"],
    "target": "ES2019",
    "module": "CommonJS",
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "newLine": "lf",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "declaration": true,
    "outDir": "dist"
  }
}
